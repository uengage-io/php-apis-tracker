{
    "name": "uengage/curl-tracker",
    "description": "Automatically track cURL API response times and publish metrics to AWS CloudWatch or Prometheus PushGateway with zero code changes",
    "type": "library",
    "license": "MIT",
    "version": "4.0.0",
    "keywords": ["curl", "api", "metrics", "cloudwatch", "aws", "prometheus", "pushgateway", "monitoring", "performance"],
    "authors": [
        {
            "name": "Your Name",
            "email": "your.email@example.com"
        }
    ],
    "require": {
        "php": ">=7.1",
        "ext-curl": "*",
        "ext-json": "*"
    },
    "require-dev": {
        "aws/aws-sdk-php": "^3.0"
    },
    "suggest": {
        "ext-uopz": "Required for zero-code-change automatic cURL hooking with CurlHook",
        "aws/aws-sdk-php": "Required when using CloudWatch backend (^3.0)"
    },
    "autoload": {
        "psr-4": {
            "CurlTracker\\": "src/"
        },
        "files": [
            "src/Helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "CurlTracker\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "phpunit",
        "check-compatibility": "@php bin/check-compatibility.php"
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}